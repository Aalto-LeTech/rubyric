<%= @course.code %> <%= @course.name %>

<% if @group.max_size > 1 %><%=t '.you_have_been_added' %>
<% @group.group_members.each do |member| %>  <%= member.user ? member.user.name : member.email %>
<% end %>

<% end %><%=t '.please_confirm' %>

<%= submit_url(:exercise => @exercise_id, :member_token => @group_member.access_token, :protocol => 'https://') %>

<% if @course_instance.submission_policy == 'unauthenticated' -%>
<%=t '.view_submissions_hint' %>
<% end -%>
