<!-- exercises/upload.html.erb -->

<div class="page-header">
  <h1>Batch upload submissions</h1>
</div>

<p class="hint">Upload a zip with submissions. Filenames should correspond to studentnumbers.</p>

<%= form_tag batch_upload_exercise_submissions_path(:exercise_id => @exercise.id), :multipart => true do %>
  <p><%= file_field_tag :file %></p>
  <p><%= submit_tag 'Upload', {:class => 'btn btn-primary'} %></p>
<% end %>

<%= link_to 'Back', exercise_path(@exercise) %>
